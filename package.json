{
  "name": "@match-app/app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npx jetify",
    "start": "react-native start --reset-cache",
    "test": "../../node_modules/.bin/jest",
    "test:coverage": "../../node_modules/.bin/jest --coverage",
    "lint": "../../node_modules/.bin/eslint .",
    "env:local": "cp envs/local.json env.json && cp config/local/google-services.json android/app/google-services.json && cp config/local/GoogleService-Info.plist ios/GoogleService-Info.plist",
    "env:staging": "cp envs/staging.json env.json && cp config/staging/google-services.json android/app/google-services.json && cp config/staging/GoogleService-Info.plist ios/GoogleService-Info.plist",
    "env:dev": "cp envs/development.json env.json && cp config/dev/google-services.json android/app/google-services.json && cp config/dev/GoogleService-Info.plist ios/GoogleService-Info.plist",
    "env:prod": "cp envs/production.json env.json && cp config/prod/google-services.json android/app/google-services.json && cp config/prod/GoogleService-Info.plist ios/GoogleService-Info.plist",
    "ios:pod": "cd ios/ && pod install",
    "android": "react-native run-android",
    "android:debug": "react-native run-android --variant=developmentDebug",
    "android:debug:device": "react-native run-android --variant=developmentDebug --deviceId=",
    "android:local:debug": "npm run env:local && react-native run-android --variant=developmentDebug",
    "android:dev:debug": "npm run env:dev && react-native run-android --variant=developmentDebug",
    "android:staging:debug": "npm run env:staging && react-native run-android --variant=developmentDebug",
    "ios:local:debug": "npm run env:local && react-native run-ios --scheme Development --configuration 'Debug(Development)'",
    "ios:dev:debug": "npm run env:dev && react-native run-ios --scheme Development --configuration 'Debug(Development)'",
    "ios:staging:debug": "npm run env:staging && react-native run-ios --scheme Development --configuration 'Debug(Development)'"
  },
